{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off",
        "no-useless-return": "off",
        "jsx-a11y/anchor-is-valid": "off"
    }
}
